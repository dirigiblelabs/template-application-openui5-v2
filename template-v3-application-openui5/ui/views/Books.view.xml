<mvc:View
	height="100%"
	controllerName="cockpit2.controllers.Books"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page
		showHeader="false"
		enableScrolling="false" >
		<TileContainer
			id="container"
			tileDelete="handleTileDelete"
			tiles="{Books>/}">
			<StandardTile
				icon="{Books>icon}"
				type="{Books>type}"
				number="{Books>number}"
				numberUnit="{Books>numberUnit}"
				title="{Books>title}"
				info="{Books>info}"
				infoState="{Books>infoState}" />
		</TileContainer>
	</Page>
</mvc:View>
